# SIP Server Configuration
NODE_ENV=development

# SIP Server Settings
SIP_PORT=5060
SIP_HOST=0.0.0.0
SERVER_ADDRESS=192.168.0.42

# API Server Settings
API_PORT=3000
API_HOST=0.0.0.0

# RTP Proxy Settings
RTP_PORT=10000
RTP_HOST=0.0.0.0

# Extension Range
EXT_MIN=100
EXT_MAX=110

# Logging Settings
LOG_LEVEL=info
LOG_FILE=./logs/sip-server.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# Security Settings
ENABLE_AUTH=false
AUTH_SECRET=your-secret-key-here
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Timeout Settings
CALL_SETUP_TIMEOUT=30000
REGISTRATION_TIMEOUT=3600
CLEANUP_INTERVAL=60000 